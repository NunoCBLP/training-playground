pipeline {
    agent none
    
    	stages {
    		stage('maven') {
			dockerfile {
				filename '04 - Dokerfile- Multiple Agents using Dockerfile'
				dir '003 - Defining Execution Environments'

		}
		steps {
			sh "mvn --version"
		}
		
		stage('node') {
			dockerfile {
				filename '04 - Dokerfile- Multiple Agents using Dockerfile'
				dir '003 - Defining Execution Environments'

		}
		steps {
			sh "npm --version"
		}
			
    	}
	
    }
}
