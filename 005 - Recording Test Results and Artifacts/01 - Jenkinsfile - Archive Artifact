pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
	
    stages {
        stage('build') {
            steps {
                sh "curl www.google.com > googlePage.html "
		
            }
        }
    }
    post {
    	always {
		archiveArtifacts artifacts:
	}
    }
}
