pipeline {
    agent { 
		docker { 
			image 'alpine/curl:latest' 
		}
	}
	
    stages {
        stage('build') {
	
            steps {
                sh "curl www.google.com > googlePage.html "
		
            }
        }
    }
        post {
    	always {
		archiveArtifacts artifacts: '/var/jenkins_home/workspace/usr8_workspace/Terceiro dia/Ex1/googlePage.html'
	}
    }

}
